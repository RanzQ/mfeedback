{
  "noppa_url": "http://noppa.aalto.fi", 
  "courses_url": "/noppa/kurssit/",
  "course_url": "/noppa/kurssi/",

  "logging": {
    "version": 1,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - [%(name)s] - %(levelname)s - %(message)s"
      }
    },

    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "simple"
      }
    },

    "loggers": {
      "scraper": {
        "level": "DEBUG",
        "handlers": ["console"],
        "propagate": "True"
      },
      "root": {
        "level": "DEBUG",
        "handlers": ["console"]
      }
    }
  }
}
